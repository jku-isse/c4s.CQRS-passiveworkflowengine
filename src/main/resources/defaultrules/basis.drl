package defaultrules

import impactassessment.api.Events.UpdatedArtifactsEvt
import passiveprocessengine.instance.ConstraintTrigger



rule "RemoveConstraintTrigger"
	salience -1000
	when
		$ct : ConstraintTrigger()
	then
		delete($ct);
end

rule "RemoveArtifactUpdateEvent"
	salience 2000
	when
		$aue : UpdatedArtifactsEvt()
	then
		delete($aue);
end
